{
    "name": "Ubuntu 20.04 DevContainer with Nested Virt",
    "dockerFile": "Dockerfile",
    "context": ".",
    "workspaceFolder": "/",
    "extensions": [
        "ms-vscode.cpptools",
        "ms-python.python"
    ],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "runArgs": [
        "--privileged",
        "--init"
    ],
    "remoteUser": "root"
}
